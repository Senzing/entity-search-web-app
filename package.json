{
  "name": "@senzing/entity-search-web-app",
  "version": "1.0.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:staging": "ng serve --disableHostCheck --host 0.0.0.0",
    "start:docker": "node inject-env && node webserver",
    "start:server": "java -jar \"..\\senzing-api-server\\target\\senzing-api-server-1.7.2.jar\" -iniFile \"%LOCALAPPDATA%\\Senzing\\Workbench\\project_1\\g2.ini\" -httpPort 8080 -allowedOrigins \"*\"",
    "build": "ng build",
    "build:docker": "ng build -c docker",
    "build:sdk": "ng-packagr -p sdk-components-ng/src/subrepo.ng-package.json",
    "install:sdk": "npm install && npm run build:sdk && npm i ./dist/@senzing/sdk-components-ng",
    "watch": "rm -fR dist/@senzing/sdk-components-ng && concurrently --kill-others \"wait-on file:dist/@senzing/sdk-components-ng/public_api.d.ts && ng serve\" \"ng build @senzing/sdk-components-ng --watch\"",
    "test": "ng test",
    "test:docker": "node inject-env && ng test --watch=false --karma-config src/karma.ci.conf",
    "test:headless": "ng test --no-watch --watch=false --karma-config src/karma.ci.conf --progress=false --no-progress --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e --configuration=e2e --protractor-config=e2e/protractor.conf.js",
    "e2e:docker": "node inject-env && ng e2e --port 8080 --configuration=e2e --protractor-config=e2e/protractor.docker.conf.js",
    "e2e:headless": "ng e2e --configuration=e2e --protractor-config=e2e/protractor.ci.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/cdk": "~7.3.4",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/material": "^7.3.4",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@senzing/rest-api-client-ng": "^1.1.0",
    "@senzing/sdk-components-ng": "file:../sdk-components-ng/dist/@senzing/sdk-components-ng/senzing-sdk-components-ng-1.2.0.tgz",
    "@senzing/sdk-graph-components": "^0.1.0",
    "@types/d3": "^5.7.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "hammerjs": "^2.0.8",
    "http-proxy-middleware": "^0.19.1",
    "karma-brief-reporter": "^0.2.0",
    "karma-junit-reporter": "^1.2.0",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "~6.3.3",
    "serve-static": "^1.13.2",
    "template-literal": "^1.0.3",
    "tslib": "^1.9.0",
    "wait-on": "^3.2.0",
    "web-animations-js": "^2.3.1",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "^7.2.13",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-in-memory-web-api": "^0.8.0",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "d3": "^5.9.1",
    "fs-extra": "^7.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.12.0",
    "protractor": "~5.4.0",
    "sass-loader": "^6.0.5",
    "scss-bundle": "^2.5.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
