{
    "/api/*": {
      "target": "http://americium.local:8084/",
      "secure": false,
      "logLevel": "debug",
      "pathRewrite": {
        "^/api": ""
      }
    },
    "/admin/auth/jwt/*": {
      "target": "http://localhost:8000/jwt/",
      "secure": true,
      "logLevel": "debug",
      "pathRewrite": {
        "^/admin/auth/jwt": ""
      }
    },
    "/admin/auth/sso/*": {
      "target": "http://localhost:8000/sso/",
      "secure": true,
      "logLevel": "debug",
      "pathRewrite": {
        "^/admin/auth/sso": "/auth"
      }
    }
  }
