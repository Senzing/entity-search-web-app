{
  "/api/*": {
    "target": "${d.SENZING_API_SERVER_URL}",
    "secure": false,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api": ""
    }
  },
  "/admin/auth/jwt/*": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}/jwt/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/jwt": "/jwt/admin"
    }
  },
  "/admin/auth/sso/status": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/sso/status": "/sso/admin/status"
    }
  },
  "/admin/auth/sso/login": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/admin/auth/sso/login": "/sso/admin/login"
    }
  },
  "/auth/jwt/*": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}/jwt/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/auth/jwt": "/jwt"
    }
  },
  "/auth/sso/*": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}/sso/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/auth/sso": "/sso"
    }
  },
  "/config/auth": {
    "target": "${d.SENZING_WEB_SERVER_ADMIN_AUTH_PATH}/conf/auth/",
    "secure": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/config/auth": ""
    }
  }
}
